		<servlet-name>Patlogin</servlet-name>
		<servlet-class>ConnectionFiles.PatientLogIn</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Patlogin</servlet-name>
		<url-pattern>/patientlogin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Doclogin</servlet-name>
		<servlet-class>ConnectionFiles.DocLogIn</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Doclogin</servlet-name>
		<url-pattern>/doclogin</url-pattern>
	</servlet-mapping>

	<servlet>
