	</servlet>

	<servlet-mapping>
		<servlet-name>Book</servlet-name>
		<url-pattern>/bookapp</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Mjpjay</servlet-name>
		<servlet-class>ConnectionFiles.MJPJAY</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Mjpjay</servlet-name>
		<url-pattern>/yoj</url-pattern>
	</servlet-mapping>



</web-app>

