		<servlet-class>ConnectionFiles.AdminLogIn</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Adminlogin</servlet-name>
		<url-pattern>/admlogin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Adminsignup</servlet-name>
		<servlet-class>ConnectionFiles.AdminSignUp</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Adminsignup</servlet-name>
		<url-pattern>/admsignup</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Docsignup</servlet-name>
